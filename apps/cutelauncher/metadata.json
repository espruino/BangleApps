        {
            "name": "cutelauncher.img",
            "url": "app-icon.js",
            "evaluate": true
        },
        {
            "name": "cutelauncher.settings.js",
            "url": "settings.js"
        }
    ],
    "data": [
        {
            "name": "cutelauncher.settings.json"
        }
