{
    "id": "cutelauncher",
    "name": "Cute Launcher",
    "shortName": "Cute Launcher",
    "version": "0.01",
    "description": "A simple launcher app for Bangle.js 2 that makes use of the full touchscreen",
    "icon": "app.png",
    "type": "launch",
    "tags": "tool,system,launcher",
    "screenshots": [
        {
            "url": "screenshot.png"
        }
    ],
    "supports": [
        "BANGLEJS2"
    ],
    "storage": [
        {
            "name": "cutelauncher.app.js",
            "url": "app.js"
