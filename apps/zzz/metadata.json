  "icon": "app.png",
  "tags": "outdoors,gps,osm",
  "supports": ["BANGLEJS","BANGLEJS2"],
  "screenshots": [{"url":"screenshot.png"}],
  "interface": "interface.html",
  "storage": [
    {"name":"zzz","url":"zzz.js"},
    {"name":"zzz.app.js","url":"app.js"},
    {"name":"zzz.img","url":"app-icon.js","evaluate":true}
  ], "data": [
    {"name":"zzz.json"},
    {"wildcard":"zzz.*.json"},
    {"wildcard":"zzz.*.img"}
  ]
}
